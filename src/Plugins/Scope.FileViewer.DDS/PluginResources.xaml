  xmlns:l="clr-namespace:Scope.FileViewer.DDS">

  <DataTemplate
    DataType="{x:Type l:DdsFileViewer}">
    
    <ScrollViewer>

      <ContentControl
        Content="{Binding Image}"/>
      
    </ScrollViewer>
    
  </DataTemplate>

</ResourceDictionary>